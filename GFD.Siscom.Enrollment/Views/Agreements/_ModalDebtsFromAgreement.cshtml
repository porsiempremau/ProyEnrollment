
<div class="modal fade" id="modalDebtAgreement" role="dialog">
    <div class="modal-dialog" style="max-width: 100%; width:50%;">
        <div class="modal-content">
            <div class="modalHeader" style="background-color:#036681;padding: 1rem;">
                <div class="row">
                    <div class="col-xs-6 col-sm-7 col-md-10 col-lg-10">
                        <p id="TitleModal" class="subtitle bold white-color" style="margin-bottom: 0px;">Deuda</p>
                    </div>
                    <div class="col-xs-6 col-sm-5 col-md-2 col-lg-2" style="float:right;">
                        <button type="button" style="color: white;" class="close" data-dismiss="modal">&times;</button>
                    </div>
                </div>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12" style="height:31vh; overflow-y:auto;">
                        <table style="width: 100%;" id="TableDebtsFromAgreement" class="table table-hover table-striped table-bordered">
                            <thead class="style-theads">
                                <tr>
                                    <th>Periodo</th>
                                    <th>Tipo</th>
                                    <th>Monto</th>
                                    <th>Descuento</th>
                                    <th>En Cuenta</th>
                                    <th>Expiración</th>
                                    <th>Detalles</th>
                                </tr>
                            </thead>
                            <tbody id="tbodyDebtsFromAgreement"></tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                @*<button type="button" class="btn btn-GFD" onclick="AddDebtAgua()">Aceptar</button>*@
                <button type="button" class="btn btn-GFD" data-dismiss="modal">Aceptar</button>
            </div>
        </div>
    </div>
</div>



