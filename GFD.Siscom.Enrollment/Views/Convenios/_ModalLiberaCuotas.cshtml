


<div class="modal fade" id="modalLiberaCuotas" role="dialog">
    <div class="modal-dialog" style="max-width: 100%; width: 37%;">
        <div class="modal-content">
            <div class="modalHeader color-header-modals-gfd">
                <div class="row">
                    <div class="col-xs-6 col-sm-7 col-md-10 col-lg-10">
                        <p id="TitleModal" class="subtitle bold white-color" style="margin-bottom: 0px;">Detalles del Convenio</p>
                    </div>
                    <div class="col-xs-6 col-sm-5 col-md-2 col-lg-2" style="float:right;">
                        <button type="button" style="color: white;" class="close" data-dismiss="modal">&times;</button>
                    </div>
                </div>
            </div>
            <div class="modal-body">
                <br />
                <div class="row">
                    <div class="col-md-12">
                        <input type="hidden" id="agreementIdModalLiberacionCuota"/>
                        <table id="TableConvenioDetails" style="width: 100%;" class="table table-hover table-striped table-bordered">
                            <thead class="style-theads">
                                <tr>
                                    <th>No. Pago</th>
                                    <th>Importe</th>
                                    <th>I.V.A.</th>
                                    <th>En Cuenta</th>
                                    <th>Status</th>
                                    <th>Calendario</th>
                                    <th>Liberación</th>
                                    <th>Fecha Pago</th>
                                </tr>
                            </thead>
                            <tbody id="tbodyConvenioDetails"></tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="btnModalSimulationGeneraDeuda" class="btn btn-GFD" onclick="LiberarCuota(document.getElementById('agreementIdModalLiberacionCuota').value)">Liberar Pago</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
            </div>
        </div>
    </div>
</div>
